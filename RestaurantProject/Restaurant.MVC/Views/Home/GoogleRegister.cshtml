@model List<Microsoft.AspNetCore.Authentication.AuthenticationScheme>
@{
    ViewData["Title"] = "GoogleRegister";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="row">
    <h2>Register</h2>
    <div class="card">
        @if(Model.Count>0)
        {
            <form asp-action="GoogleLogin" asp-controller="auth" method="post">

                <div class="">
                    @foreach (var provider in Model)
                    {
                        <button class="btn btn-primary text-center" type="submit" name="provider" value="@provider.Name">@provider.DisplayName</button>
                    }

                </div>


            </form>
        }
    </div>


</div>